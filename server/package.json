{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write ./src",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "cross-env NODE_ENV=production node dist/main",
    "lint-check": "prettier --check ./src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy-nft": "hardhat run src/scripts/deploy-nft.js",
    "deploy-market": "hardhat run src/scripts/deploy-marketplace.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.53.1",
    "@aws-sdk/s3-presigned-post": "^3.53.1",
    "@metamask/eth-sig-util": "^4.0.0",
    "@nestjs/apollo": "^10.0.6",
    "@nestjs/common": "^8.1.1",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.1.1",
    "@nestjs/graphql": "^10.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/typeorm": "^8.0.3",
    "@types/form-data": "^2.5.0",
    "@types/graphql-upload": "^8.0.11",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "@udia/graphql-postgres-subscriptions": "^1.1.0",
    "apollo-server-express": "^3.6.3",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "form-data": "^4.0.0",
    "graphql": "^16.3.0",
    "graphql-postgres-subscriptions": "^1.0.5",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^13.0.0",
    "graphql-ws": "^5.6.3",
    "image-size": "^1.0.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "typeorm": "^0.2.45",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "ethers": "^5.6.0",
    "husky": "^4.3.6",
    "jest": "^27.3.0",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,js,css,json,md}": [
      "prettier --write"
    ]
  }
}
